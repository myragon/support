{
    "name": "myragon/support",
    "description": "Package with common myragon components",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "csfix": "./vendor/bin/php-cs-fixer fix src",
        "phpstan": "./vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "homepage": "https://github.com/myragon",
    "autoload": {
        "psr-4": {
            "Myragon\\Support\\": "src/"
        }
    },
    "authors": [
        {
            "name": "bdumitrua",
            "email": "bdumitrua@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.84",
        "phpstan/phpstan": "^2.1"
    }
}